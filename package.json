{
    "name": "vite-project",
    "author": {
        "name": "FE Viu Chieu PHim"
    },
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "build": "vite build",
        "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "format": "prettier -l --write \"src/**/*.{html,css,js,jsx}\""
    },
    "dependencies": {
        "antd": "^5.20.0",
        "axios": "^1.7.3",
        "moment": "^2.30.1",
        "prettier": "^3.3.3",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-router-dom": "^6.26.0"
    },
    "devDependencies": {
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@vitejs/plugin-react-swc": "3.5.0",
        "eslint": "8.57.0",
        "eslint-plugin-react": "7.34.2",
        "eslint-plugin-react-hooks": "4.6.2",
        "eslint-plugin-react-refresh": "0.4.7",
        "sass": "^1.77.8",
        "vite": "5.3.1"
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "jsxSingleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "always",
        "endOfLine": "lf",
        "htmlWhitespaceSensitivity": "css"
    }
}
